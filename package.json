    "rxjs": "^6.5.2"
  },
  "devDependencies": {
    "@co.mmons/node-utils": "git+https://github.com/co-mmons/node-utils.git",
    "bignumber.js": "^9.0.0",
    "ng-packagr": "^5.5.1",
    "ts-node": "^8.4.1",
    "typescript": "~3.4.0",
    "zone.js": "^0.9.1"
  },
